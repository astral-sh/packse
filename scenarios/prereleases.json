[
    {
        "name": "root-wants-prerelease",
        "description": "The user wants a prerelease",
        "root": {
            "requires": [
                "a>=1.0.0-beta.1,<2.0.0"
            ]
        },
        "packages": {
            "a": {
                "versions": {
                    "1.0.0-beta.1": {}
                }
            }
        }
    },
    {
        "name": "prerelease-compatibility-1",
        "description": "The user wants a prerelease, but there's also a more recent prerelease with the same release segment",
        "root": {
            "requires": [
                "a>=1.0.0-beta.1,<3.0.0"
            ]
        },
        "packages": {
            "a": {
                "versions": {
                    "1.0.0-beta.1": {},
                    "1.0.0-beta.2": {}
                }
            }
        }
    },
    {
        "name": "prerelease-compatibility-2",
        "description": "The user wants a prerelease, but there's also a more recent prerelease with a different release segment",
        "root": {
            "requires": [
                "a>=1.0.0-beta.1,<3.0.0"
            ]
        },
        "packages": {
            "a": {
                "versions": {
                    "1.0.0-beta.1": {},
                    "2.0.0-beta.1": {}
                }
            }
        }
    },
    {
        "name": "transitive-prerelease-dependency",
        "description": "Inspired by msgraph-sdk 1.0.0 -> msgraph-core 1.0.0-alpha.4, we have a transitive dependency on a prerelease",
        "root": {
            "requires": [
                "a"
            ]
        },
        "packages": {
            "a": {
                "versions": {
                    "1.0.0": {
                        "requires": [
                            "b==1.0.0-beta.1"
                        ]
                    }
                }
            },
            "b": {
                "versions": {
                    "1.0.0-beta.1": {}
                }
            }
        }
    },
    {
        "name": "transitive-prerelease-and-stable-dependency",
        "description": "A transitive dependency has both a prerelease and a stable selector, but can only be satisfied by a prerelease",
        "root": {
            "requires": [
                "a",
                "b"
            ]
        },
        "packages": {
            "a": {
                "versions": {
                    "1.0.0": {
                        "requires": [
                            "c==2.0.0-beta.1"
                        ]
                    }
                }
            },
            "b": {
                "versions": {
                    "1.0.0": {
                        "requires": [
                            "c>=1.0.0,<=3.0.0"
                        ]
                    }
                }
            },
            "c": {
                "versions": {
                    "1.0.0": {},
                    "2.0.0-beta.1": {}
                }
            }
        }
    },
    {
        "name": "more-recent-prerelease",
        "description": "A dependency can be satisfied by a stable release, but there is a more recent prerelease",
        "root": {
            "requires": [
                "a"
            ]
        },
        "packages": {
            "a": {
                "versions": {
                    "1.0.0": {},
                    "2.0.0-beta.1": {}
                }
            }
        }
    }
]
